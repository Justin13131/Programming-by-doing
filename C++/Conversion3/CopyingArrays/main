#include <iostream>
#include <ctime>
#include <time.h> 
#include <algorithm>

using namespace std;

int main()
{
    srand(time(NULL));

    int arr1[10] = {};   
    int arr2[10] = {};
    for (int i = 0; i < 10; i++) {
        int randomnumber = rand() % 100 + 1;
        arr1[i] = randomnumber;
        arr2[i] = randomnumber; 
    }  
    arr1[9] = -7;

    for(int d=0; d<10; d++){
		cout << arr1[d] << "  ";
	}
	cout << endl; 
    for(int b=0; b<10; b++){
		cout << arr2[b] << "  ";
	}
    
}


 